/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=b,b=sel,out=d);
    Nand(a=sel,b=sel,out=notsel);
    Nand(a=notsel,b=a,out=e);
    Nand(a=d,b=e,out=out);

}